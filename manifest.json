{
  "manifest_version": 3,
  "name": "Decent",
  "version": "0.0.1",
  "description": "A simple Chrome extension that encrypts the text of the webpage",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "decent.png"
  },
  "permissions": ["activeTab", "contextMenus", "storage", "scripting"],
  "icons": {
    "128": "decent.png"
  }
}